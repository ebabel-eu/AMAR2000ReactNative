// Configuration: http://eslint.org/docs/user-guide/configuring
{
  "env": {
    "browser": false,
    "es6": true,
    "commonjs": true
  },
  "rules": {
    "react/jsx-first-prop-new-line": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "0.14.8"
    }
  },
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ]
}